{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    //gin -i   -a 5001  -p 5000    run  main.go    
    //gin  -a 5001  -p 5000    run  main.go   
    "version": "2.0.0",
    "tasks": [
        {
            "label": "front-api run",
            "type": "shell",
            "command": " gin --immediate --appPort 5001 -port 5000  run  main.go  ",
            "args": [],
            "problemMatcher": [
                "$tsc"
            ],
            "presentation": {
                "reveal": "always"
            },
            "options": {
                "cwd": "${workspaceFolder}/backend/api"
            },
            "group": "none"
        }
    ]
}